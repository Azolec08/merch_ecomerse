{
  "name": "template-next",
  "version": "0.1.0",
  "private": true,
  "contributors": [
    {
      "name": "Rodrick Alcantara",
      "url": "https://github.com/constROD"
    }
  ],
  "description": "Template Next",
  "author": "Rodrick Alcantara <rialcantara21@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "next lint",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --write './src/**/*.ts{,x}' './app/**/*.ts{,x}'",
    "check:types": "tsc",
    "check:spell": "git diff --name-only | npx cspell --no-summary --no-progress --no-must-find-files --file-list stdin",
    "check:all": "concurrently \"pnpm lint\" \"pnpm check:types\" \"pnpm check:spell\"",
    "pre-commit": "concurrently \"lint-staged --allow-empty\" \"pnpm check:spell\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@reduxjs/toolkit": "^2.3.0",
    "@tanstack/react-query": "^5.18.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "immer": "^10.0.3",
    "lucide-react": "^0.322.0",
    "next": "14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.0",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@tanstack/react-query-devtools": "^5.18.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.53",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.2",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cspell": "^8.3.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "husky": "^9.0.10",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.33",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "packageManager": "pnpm@8.14.1"
}
